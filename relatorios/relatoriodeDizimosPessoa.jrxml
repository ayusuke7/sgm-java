	<queryString>
		<![CDATA[select * from (dizimos
inner join membros
on dizimos.id_membro = membros.id_membro)
inner join igrejas
on dizimos.id_igreja = igrejas.id_igreja
where membros.id_membro = $P{IdMembro}
order by cdate(mes_ref)]]>
	</queryString>
