	<queryString>
		<![CDATA[SELECT Dizimos.Id, Membros.Nome, MesRef,
Dizimo, Oferta, Igrejas.Nome AS NomeIgreja,
Texto, Cnpj, Igrejas.Contatos
FROM (Dizimos INNER JOIN Membros
ON Dizimos.IdMembro = Membros.Id)
INNER JOIN Igrejas
ON Dizimos.IdIgreja = Igrejas.Id
